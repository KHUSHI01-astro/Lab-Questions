#include <stdio.h>

int main() {
    int n, i, j, maxCount = 0, count, mostFreq;

    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    for (i = 0; i < n; i++) {
        count = 0;
        for (j = 0; j < n; j++) {
            if (arr[i] == arr[j]) {
                count++;
            }
        }

        if (count > maxCount) {
            maxCount = count;
            mostFreq = arr[i];
        }
    }

    printf("\nThe most frequent element is: %d\n", mostFreq);
    printf("It appears %d times.\n", maxCount);

    return 0;
}
